{
  "name": "4z-site",
  "version": "1.0.0",
  "description": "A minimal static site with blog, projects, and changelog. AI generated.",
  "main": "index.js",
  "scripts": {
    "build": "eleventy",
    "build:prod": "ELEVENTY_ENV=production eleventy",
    "serve": "eleventy --serve",
    "clean": "rm -rf docs",
    "generate-og": "node scripts/generate-og-images.js",
    "build:with-og": "npm run generate-og && npm run build:prod"
  },
  "keywords": [
    "11ty",
    "blog",
    "static-site"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "dependencies": {
    "glob": "^7.2.3",
    "markdown-it": "^14.1.0",
    "node-html-to-image": "^5.0.0",
    "prismjs": "^1.29.0",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6"
  }
}
